{
  "expo": {
    "name": "chatting-app",
    "slug": "chatting-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#1E2732"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.nmn.chattingapp",
      "infoPlist": {
        "CFBundleURLTypes" : [
          {
            "CFBundleURLSchemes": ["com.googleusercontent.apps.618767418010-1kgchjbbdl40ubed4uck7voqeof7hekj"]
          }
        ]
      }
    },
    "android": {
      "googleServicesFile" : "./android/app/google-services.json",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.nmn.chattingapp",
      "permissions": ["android.permission.RECORD_AUDIO"]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "@react-native-google-signin/google-signin",
        {
          "iosUrlScheme" : "com.googleusercontent.apps.618767418010-1kgchjbbdl40ubed4uck7voqeof7hekj"
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "Allow $(PRODUCT_NAME) to access your photos",
          "cameraPermission": "Allow $(PRODUCT_NAME) to open the camera",
          "microphonePermission": "Allow $(PRODUCT_NAME) to access your microphone"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "tsconfigPaths": true
    },
    "extra": {
      "eas": {
        "projectId": "ed14363b-8195-476b-8384-b7a6633f730a"
      }
    }
  }
}
